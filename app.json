{
  "expo": {
    "name": "Sub2Me",
    "slug": "Sub2Me",
    "version": "1.0.0",
    "scheme": "Sub2Me",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/sub2me-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f036e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nivanshotchkiss.Sub2Me"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nivanshotchkiss.Sub2Me"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3d36c038-472c-405e-9925-4875308b7fb4"
      }
    }
  }
}
